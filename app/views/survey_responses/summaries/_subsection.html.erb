<div class="panel panel-default">
  <h3 class="panel-heading panel-title"><%= subsection.title %></h3>

<table class="table">
  <% subsection.values_for(@survey_response).each do |v| %>
  <tr>
    <td><%= v.question.label %></td>
    <td><%= v.value %></td>
  </tr>
  <% end %>
  <% if subsection.totalable? %>
    <tr>
      <td><strong>Subsection Total:</strong></td>
      <td><strong><%= @survey_response.values_in_subsection(subsection).total %></strong></td>
    </tr>
  <% end %>
</table>
</div>