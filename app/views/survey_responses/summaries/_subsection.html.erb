<div class="panel panel-default">
  <div class="panel-heading"><%= subsection.title %></div>

<table class="table">
  <% subsection.values_for(@survey_response).each do |v| %>
  <tr>
    <td><%= v.question.label %></td>
    <td><%= v.value %></td>
  </tr>
  <% end %>
  <% if subsection.totalable? %>
    <tr>
      <td>Subsection Total:</td>
      <td><%= @survey_response.values_in_subsection(subsection).total %></td>
    </tr>
  <% end %>
</table>
</div>