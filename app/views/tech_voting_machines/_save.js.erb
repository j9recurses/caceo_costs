$("ul.errors").html("")
<% if @tech_voting_machine.errors.any? %>
  <% @tech_voting_machine.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".tech-index").html("<%= escape_javascript(render 'index') %>")
  $("#tech-modal").modal("hide")
<% end %>
